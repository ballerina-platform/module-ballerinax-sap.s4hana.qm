        "responses": {
          "200": {
            "description": "Retrieved entities",
            "content": {
              "application/json": {
                "schema": {
                  "title": "CollectionOfA_InspectionPlanWrapper",
                  "type": "object",
                  "properties": {
                    "d": {
                      "title": "CollectionOfA_InspectionPlan",
                      "type": "object",
                      "properties": {
                        "__count": {
